{
  "version": 2,
  "builds": [
    {
      "src": "rag/rag_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "rag/rag_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
